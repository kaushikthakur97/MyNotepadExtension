{
  "manifest_version": 3,
  "name": "My Tabbed Notepad",
  "version": "1.0",
  "description": "A lightweight and effective notepad with tabs.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  }
}